    "glob-parent": "5.1.2",
    "lodash": "4.17.21",
    "mocha": "7.1.2",
    "node-fetch": "2.6.7",
    "normalize-url": "4.5.1",
    "path-parse": "1.0.7",
    "tar": "4.4.18",
